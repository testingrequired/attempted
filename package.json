{
  "name": "@testingrequired/attempted",
  "version": "0.0.1",
  "description": "Typed error handling for functions",
  "main": "dist/index.umd.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest typecheck --run && vitest run --coverage",
    "build": "vite build && npm run docs",
    "verify": "npm run test && npm run build",
    "docs": "npx typedoc --plugin typedoc-plugin-missing-exports src/index.ts"
  },
  "engines": {
    "node": "^16.18.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "0.33.0",
    "typedoc-plugin-missing-exports": "2.0.0",
    "typescript": "^4.9.4",
    "vite": "4.4.4",
    "vite-plugin-dts": "3.3.1",
    "vitest": "0.33.0"
  },
  "files": [
    "dist"
  ]
}
